{
  "compileOnSave": true,
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "target": "es2015",
    "sourceMap": true,
    "sourceRoot": "express_server/src",
    "outDir": "express_server/dist"
  },
  "include": [
    "express_server/src"
  ],
  "exclude": [
    "express_server/dist",
    "node_modules"
  ]
}




