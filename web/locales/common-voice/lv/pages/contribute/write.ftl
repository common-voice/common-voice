    { $totalSentences ->
        [zero] Savākts { $uploadedSentences } no 1 teikuma
        [one] Savākti { $uploadedSentences } no { $totalSentences } teikumiem
       *[other] Savākti { $uploadedSentences } no { $totalSentences } teikumiem
    }
