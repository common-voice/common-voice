## SMALL BATCH SUBMISSION

# <icon></icon> will be replaced with an icon that represents writing a sentence
small-batch-instruction = <icon></icon> Mweekhezele farazi zikina ancuwaye atthu awaatta
multiple-sentences-error = Nhivinyari farazi saawatta vaara vamoote
exceeds-small-batch-limit-error = Khintakheya winviyari farazi zoovikana alufu
# $retryLimit represents the amount of time in minutes a user has to wait to retry an upload
rate-limit-toast-message-minutes =
    { $retryLimit ->
        [one] Zavikana okhupi zawinviyariwa. Nweecele minuutu moote mmananise theenya
       *[other] Zavikana okhupi zawinviyariwa. Mmananise theenya { $retryLimit } minuutu
    }
# $retryLimit represents the amount of time in seconds a user has to wait to retry an upload
rate-limit-toast-message-seconds =
    { $retryLimit ->
        [one] Zavikana okhupi zawinviyariwa. Nweecele sekuntu moote mmananise theenya
       *[other] Zavikana okhupi zawinviyariwa. Mmananise theenya { $retryLimit } sekuntu
    }
# $retryLimit represents the amount of time in minutes a user has to wait to retry an upload
rate-limit-message-minutes =
    { $retryLimit ->
        [one] Weeyo wavikana okhupi yoori mwinviyari pagina eti. Mwecele minuutu mmoote ihinaaze winviyari farasi kiina. Naxhukhurela mwaasa woovilela!
       *[other] Weeyo wavikana okhupi yoori mwinviyari pagina eti. Mwecele{ $retryLimit }  minuutu ihinaaze winviyari farasi kiina Naxhukhurela mwaasa woovilela!
    }
# $retryLimit represents the amount of time in seconds a user has to wait to retry an upload
rate-limit-message-seconds =
    { $retryLimit ->
        [one] Weeyo wavikana okhupi yoori mwinviyari pagina eti. M,wecele sekuntu moote ihinaaze winviyari farasi kiina. Naxhukhurela mwaasa woovilela!
       *[other] Weeyo wavikana okhupi yoori mwinviyari pagina eti. M,wecele{ $retryLimit } sekuutu  ihinaaze winviyari farasi kiina. Naxhukhurela mwaasa woovilela!
    }
# $uploadedSentences represents the number of sentences accepted from the small batch submission, $totalSentences represents the total number of sentences in the small batch submission
add-small-batch-success =
    { $totalSentences ->
        [one] { $uploadedSentences } Naxhuma farazi moote
       *[other] { $uploadedSentences }  { $totalSentences } naxhuma farazi
    }
# $uploadedSentences represents the number of sentences accepted from the small batch submission, $totalSentences represents the total number of sentences in the small batch submission
small-batch-response-message =
    { $totalSentences ->
        [one] { $uploadedSentences } Yaxhumiwa farasi moote. Ntthitte <downloadLink>apha</downloadLink> Para opaxari farazi zookatthaliwa.
       *[other] { $uploadedSentences } de { $totalSentences } farazi zooxhumiwa. Ntthitte <downloadLink>apha</downloadLink> Para opaxari farazi zookatthaliwa.
    }
small-batch-sentences-rule-1 = Mwilotte tarikhi “Keekezele na farazi piivi?”
small-batch-sentences-rule-2 = Khula liinya weekezele na farazi moote
small-batch-sentences-rule-3 = Mwakanyule farazi ziriye liinya nttithaka “Enter” wala “Return” cooxhi moote
small-batch-sentences-rule-4 = Mwekezele mpakha farazi alufu
small-batch-sentences-rule-5 = Farazi zooxhi zinaye olupa vithu zoolattana
small-batch-sentences-rule-6 = Farazi zooxhi zinaye wiiyana ocitariwa mananna mamote
