## SMALL BATCH SUBMISSION

# <icon></icon> will be replaced with an icon that represents writing a sentence
small-batch-instruction = <icon></icon> Vulgi dida gnolo ma sumun wuma
multiple-sentences-error = Nigi mè asa vul dida gnol di hai ma hot ma kep di
exceeds-small-batch-limit-error = Nigi mè asa vul ha 1000 dida di
# $retryLimit represents the amount of time in minutes a user has to wait to retry an upload
rate-limit-toast-message-minutes =
    { $retryLimit ->
        [one] Hot valam aseya . Hodom lum hai 1 ma gnama
       *[other] Hot valam aseya . Hodom lum hai { $retryLimit } ma gnama
    }
# $retryLimit represents the amount of time in seconds a user has to wait to retry an upload
rate-limit-toast-message-seconds =
    { $retryLimit ->
        [one] Hot valam aseya. Hodom lum hai 1 ma gnam nèna
       *[other] Hot valam aseya. Hodom lum hai { $retryLimit } ma gnam nèna
    }
# $retryLimit represents the amount of time in minutes a user has to wait to retry an upload
rate-limit-message-minutes =
    { $retryLimit ->
        [one] Nigi tcho vadeya hai hotna fi derwela samgi duaha  1 ma gnama   busavata nigi vul dida ta wilida . Gigidebe fi ma nigi sama .
       *[other] Nigi tcho vadeya hai hotna fi derwela samgi duaha. Ma gnam { $retryLimit }busavata nigi vul dida ta wilida . Gigidebe fi ma nigi sama.
    }
# $retryLimit represents the amount of time in seconds a user has to wait to retry an upload
rate-limit-message-seconds =
    { $retryLimit ->
        [one] Nigi tcho vadeya hai hotna fi derwela samgi duaha 1 ma gnam nèna   busavata nigi vul dida ta wilida . Gigidebe fi ma nigi sama.
       *[other] Nigi tcho vadeya hai hotna fi derwela samgi duaha { $retryLimit } ma gnam nèna   busavata nigi vul dida ta wilida . Gigidebe fi ma nigi sama.
    }
# $uploadedSentences represents the number of sentences accepted from the small batch submission, $totalSentences represents the total number of sentences in the small batch submission
add-small-batch-success =
    { $totalSentences ->
        [one] { $uploadedSentences }  tapgi dida hai
       *[other] { $uploadedSentences }  tapgi dida hai { $totalSentences }
    }
# $uploadedSentences represents the number of sentences accepted from the small batch submission, $totalSentences represents the total number of sentences in the small batch submission
small-batch-response-message =
    { $totalSentences ->
        [one] { $uploadedSentences } dida ta tabaida hai 1  <downloadLink>tani</downloadLink> fi nigi sli dida ma nigi mè minim di
       *[other] { $uploadedSentences } dida ta tabaida hai { $totalSentences }<downloadLink>tani</downloadLink> fi nigi sli dida ma nigi mè minim di
    }
