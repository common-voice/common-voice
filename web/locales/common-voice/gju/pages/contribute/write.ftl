exceeds-small-batch-limit-error = ایک ہزار تے زیادہ فقرا جمع نیہہ کر سکتا
# $retryLimit represents the amount of time in minutes a user has to wait to retry an upload
rate-limit-toast-message-minutes =
    { $retryLimit ->
        [one] شرح کی حد تے اگے نکل گیو ۔دوبارہ کوشش کروں ایک منٹ ما
       *[other] شرح کی حد تے اگے نکل گیو ۔دوبارہ کوشش کروں ایک منٹ{ $retryLimit } ما
    }
# $retryLimit represents the amount of time in seconds a user has to wait to retry an upload
rate-limit-toast-message-seconds =
    { $retryLimit ->
        [one] شرح کی حد تے اگے نکل گیو ۔دوبارہ کوشش کروں ایک سیکنڈ  ما
       *[other] شرح کی حد تے اگے نکل گیو ۔دوبارہ کوشش کروں ایک سیکنڈ { $retryLimit } ما
    }
# $retryLimit represents the amount of time in minutes a user has to wait to retry an upload
rate-limit-message-minutes =
    { $retryLimit ->
        [one] تم اس صفحہ نا جمع کرن کی حد تک پہچ گیا برائے مہربانی ایک منٹ انتظار کرو ایک اگلو فقرو جمع کرن تے پہلاں ۔تھارا ا صبر کو شکریو
       *[other] تم اس صفحہ نا جمع کرن کی حد تک پہچ گیا برائے مہربانی { $retryLimit } منٹ انتظار کرو ایک اگلو فقرو جمع کرن تے پہلاں ۔تھارا ا صبر کو شکریو
    }
# $retryLimit represents the amount of time in seconds a user has to wait to retry an upload
rate-limit-message-seconds =
    { $retryLimit ->
        [one] تم اس صفحہ نا جمع کرن کی حد تک پہچ گیا برائے مہربانی ایک سیکنڈ  انتظار کرو ایک اگلو فقرو جمع کرن تے پہلاں ۔تھارا ا صبر کو شکریو
       *[other] تم اس صفحہ نا جمع کرن کی حد تک پہچ گیا برائے مہربانی { $retryLimit } منٹ انتظار کرو ایک اگلو فقرو جمع کرن تے پہلاں ۔تھارا ا صبر کو شکریو
    }
# $uploadedSentences represents the number of sentences accepted from the small batch submission, $totalSentences represents the total number of sentences in the small batch submission
add-small-batch-success =
    { $totalSentences ->
        [one] { $uploadedSentences } ایک فقرا نا جمع کرن کو
       *[other] { $uploadedSentences }فقرا نا جمع کرن کو { $totalSentences }
    }
# $uploadedSentences represents the number of sentences accepted from the small batch submission, $totalSentences represents the total number of sentences in the small batch submission
small-batch-response-message =
    { $totalSentences ->
        [one] { $uploadedSentences } ایک فقرا نا جمع کرن کو ۔ کلک
       *[other] <downloadLink>ات فقرا نا جمع کرن کو { $totalSentences } کلک
    }
