    { $retryLimit ->
        [one] Ro tappee. Prow reesht ayns grig
        [two] Ro tappee. Prow reesht ayns 2 ghig
        [few] Ro tappee. Prow reesht ayns { $retryLimit } grig
        [many] Ro tappee. Prow reesht ayns { $retryLimit } grig
