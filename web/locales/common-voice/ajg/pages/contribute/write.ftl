    { $uploadedSentences ->
        [one] { $uploadedSentences } Enyɔgbewo fɔfɔkpli dó{ $totalSentences }
       *[other] { $uploadedSentences } Enyɔgbewo fɔfɔkpli dó{ $totalSentences }
    }
